<div class="sc-TakeoverHeader-content">
  <figure class="sc-TakeoverHeader-logoContainer sc-p--xs">
    <%= link_to(image_tag('ttlogo.png', size: "47x47"), admin_root_url) %>
  </figure>
  <% if signed_in? %>
    <div class="sc-TakeoverHeader--multipleCta">
      <div class="sc-p--s">
        <%= link_to 'Splits', admin_root_path %>
      </div>
      <div class="sc-p--s">
        <%= link_to 'Log out', destroy_admin_session_path, method: :delete %>
      </div>
    </div>
  <% end %>
</div>
